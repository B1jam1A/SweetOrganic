<!DOCTYPE html>
<html>
<head>
    <title>Profil Utilisateur</title>
</head>
<body>
    <h2>Profil de <%= prenom %> <%= nom %></h2>
    <p>Email : <%= email %></p>
    <p>Téléphone : <%= telephone %></p>

    <h3>Adresses :</h3>
    <ul>
        <% adresses.forEach(function(adresse) { %>
            <li>
                <%= adresse.adresse.numero %> <%= adresse.adresse.rue %><br>
                <%= adresse.adresse.ville %>, <%= adresse.adresse.codePostal %><br>
                <%= adresse.adresse.pays %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
